<Files "sacaliens.conf">
  Order Deny,Allow
  Deny from all
</Files>

RewriteEngine   off
RewriteRule		(.*)\.css$					$1.css	[L]
RewriteRule		(.*)\.js$					$1.js	[L]
RewriteRule		(.*)\.(jpg|png|gif)$		$1.$2	[L]
RewriteRule     "^/api(.*)$"                 ng-rest.php [NC,L]
RewriteRule     (index|delog|ng-rest).php           $1.php  [L]
RewriteRule     .*							sacaliens.php	[L]
